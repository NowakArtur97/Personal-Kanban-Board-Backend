type TaskResponse {
    taskId: ID!
    title: String!,
    description: String,
    status: TaskStatus!,
    priority: TaskPriority!,
    targetEndDate: Date,

    createdBy: String!,
    createdOn: Date!,
    updatedBy: String,
    updatedOn: Date,
    assignedTo: String!,
}

enum TaskStatus {
    READY_TO_START
    IN_PROGRESS
    DONE
}

enum TaskPriority {
    LOW
    MEDIUM
    HIGH
}
